{
  "name": "architect/subtraction-service",
  "version": "0.1.0",
  "description": "Service to do the subtraction arithmetic operation",
  "keywords": [
    "architect",
    "math",
    "example"
  ],
  "author": [
    "David Thor"
  ],
  "license": "ISC",
  "dependencies": {
    "architect/addition-service": "file:../../../addition-service/rest/"
  },
  "language": "java",
  "api": {
    "type": "rest"
  },
  "datastores": {
    "primary": {
      "image": "postgres:10",
      "port": 5432,
      "parameters": {
        "POSTGRES_USER": {
          "alias": "username",
          "default": "postgres"
        },
        "POSTGRES_PASSWORD": {
          "alias": "password",
          "default": "architect"
        },
        "POSTGRES_DB": {
          "alias": "name",
          "default": "subtraction_service"
        }
      }
    }
  },
  "debug": "sh -c '/usr/src/app/bin/architect_subtraction_service'"
}
