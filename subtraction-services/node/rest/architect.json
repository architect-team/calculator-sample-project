{
  "name": "architect/subtraction-service-rest",
  "version": "0.2.0",
  "description": "Service to do the subtraction arithmetic operation",
  "keywords": [
    "architect",
    "math",
    "rest"
  ],
  "author": [
    "David Thor"
  ],
  "license": "ISC",
  "start": "npm start",
  "language": "node",
  "api": {
    "type": "rest"
  },
  "dependencies": {
    "architect/addition-service-grpc": "latest"
  },
  "parameters": {
    "ADDITION_SERVICE_ADDRESS": {
      "default": {
        "valueFrom": {
          "dependency": "architect/addition-service-grpc:latest",
          "interface": "main",
          "value": "$HOST:$PORT"
        }
      }
    },
    "PORT": {
      "default": {
        "valueFrom": {
          "value": "$PORT",
          "interface": "main",
          "self": true
        }
      }
    }
  },
  "debug": "npm run dev",
  "interfaces": {
    "main": {
      "port": 8080
    }
  }
}
