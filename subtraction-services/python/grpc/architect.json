{
  "name": "architect/subtraction-service-grpc",
  "version": "0.1.0",
  "description": "Service to do the subtraction arithmetic operation",
  "keywords": [
    "architect",
    "math",
    "example"
  ],
  "author": [
    "David Thor"
  ],
  "license": "ISC",
  "dependencies": {
    "architect/addition-service-rest": "latest"
  },
  "parameters": {
    "ADDITION_SERVICE_ADDRESS": {
      "default": {
        "valueFrom": {
          "dependency": "architect/addition-service-rest:latest",
          "value": "$HOST:$PORT"
        }
      }
    }
  },
  "language": "python",
  "api": {
    "type": "grpc",
    "definitions": [
      "./service.proto"
    ]
  },
  "debug": "watchmedo auto-restart --recursive -- python3 -u src/main.py"
}
