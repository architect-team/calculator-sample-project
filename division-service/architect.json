{
  "name": "architect/division-service-grpc",
  "dependencies": {
    "architect/subtraction-service-rest": "latest"
  },
  "parameters": {
    "SUBTRACTION_SERVICE_ADDRESS": {
      "default": {
        "valueFrom": {
          "dependency": "architect/subtraction-service-rest:latest",
          "interface": "main",
          "value": "$HOST:$PORT"
        }
      }
    },
    "PORT": {
      "default": {
        "valueFrom": {
          "value": "$PORT",
          "interface": "main"
        }
      }
    }
  },
  "datastores": {},
  "api": {
    "type": "grpc",
    "definitions": [
      "./service.proto"
    ]
  },
  "subscriptions": {},
  "version": "0.2.0",
  "description": "Example service performing the division operation",
  "keywords": [
    "architect",
    "math",
    "grpc"
  ],
  "author": [
    "David Thor"
  ],
  "license": "ISC",
  "language": "node",
  "debug": "npm run debug",
  "interfaces": {
    "main": {
      "port": 8080
    }
  }
}
